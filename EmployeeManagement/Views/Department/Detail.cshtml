@model Department

@{
    var fn = Model.Name;
    ViewData["TitlePhrase"] = fn;    
}

<h1>@fn's Detail <a href="/department/index" class="btn btn-primary">Back to All Departments</a></h1>

<ul class="list-group">
  <li class="list-group-item">Department Name : @fn</li>
  <li class="list-group-item">Code : @Model.Code</li>
  <li class="list-group-item">Description : @Model.Description</li>
  <li class="list-group-item">Start Date : @Model.StartDate</li>
</ul>